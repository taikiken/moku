    "intersection-observer": "^0.11.0",
    "promise-polyfill": "^8.1.3",
    "webpack": "^4.44.1",
    "whatwg-fetch": "^3.4.0"
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
