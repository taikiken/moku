{
  "name": "moku",
  "version": "0.5.5",
  "description": "moku emulate eventdispatcher",
  "main": "index.js",
  "directories": {
    "test": "test",
    "src": "src",
    "dist": "dist",
    "build": "build",
    "scripts": "scripts"
  },
  "scripts": {
    "dev": "node scripts/dev.js",
    "build": "node scripts/build.js",
    "js:lint": "eslint ./src/**/*.js",
    "js:lint:fix": "eslint ./src/**/*.js --fix",
    "format:js": "prettier --write 'src/**/*.{js,jsx}'",
    "jsdoc": "jsdoc src -d jsdocs --verbose -c jsdoc.config.json -r",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run format:js",
      "npm run js:lint:fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taikiken/moku.git"
  },
  "keywords": [
    "javascript",
    "es2018"
  ],
  "author": "@taikiken",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taikiken/moku/issues"
  },
  "homepage": "https://github.com/taikiken/moku#readme",
  "dependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/register": "^7.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "core-js": "^3.6.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-prettier": "^3.1.2",
    "intersection-observer": "^0.7.0",
    "promise-polyfill": "^8.1.3",
    "webpack": "^4.42.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1"
  }
}
